//Building Race
code:
#include <iostream>
using namespace std;

int main() {
	int T;
	float A,B,X,Y;
	
	cin>>T;
	
	for(int i=0;i<T;i++){
	    
	    cin>>A>>B>>X>>Y;
	    
	    float TA=A/X,TB=B/Y;
	    
	    if(TA<TB){
	        
	        cout<<"Chef"<<endl;
	    }
	  else if(TA>TB){
	        
	        cout<<"Chefina"<<endl;
	    }
	    
	    else{
	        cout<<"Both"<<endl;
	    }
	    
	}

	
	return 0;
}
